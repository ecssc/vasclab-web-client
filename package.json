{
  "name": "vasclab-web-client",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "eslint": "^3.14.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "react-scripts": "0.8.5"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^2.6.1",
    "redux-saga": "^0.14.3",
    "superagent": "^3.3.2",
    "superagent-promise-plugin": "^3.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "REACT_APP__API_URL=https://api.vasclab.test/ REACT_APP__OAUTH_CLIENT_ID=client_id react-scripts test --env=jsdom",
    "coverage": "API_URL=https://api.vasclab.test/ OAUTH_CLIENT_ID=client_id react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/eslint src/ || true"
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/config.*"
    ],
    "setupTestFrameworkScriptFile": "./__mocks__/localStorage.js"
  }
}
